@model IEnumerable<User>


<table class="table" style="margin-top: 10px">
    <tr><th>Email address</th><th></th></tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <form>
                    <a class="btn btn-primary" asp-action="Edit" asp-controller="Roles" asp-route-userid="@user.Id">Права доступу</a>
                </form>
            </td>
        </tr>
    }
</table>